{
  "name": "prompt-packs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "playwright install chromium",
    "crawl": "node crawler/crawl-openai-academy.mjs",
    "clean": "rm -rf ./crawler/out && mkdir -p ./crawler/out",
    "sync:data": "node scripts/sync-data.mjs",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "bash scripts/deploy-gh-pages.sh",
    "all": "npm run crawl && npm run data:sync && npm run dev"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "playwright": "^1.48.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
